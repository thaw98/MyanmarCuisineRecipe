<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myanmarcuisinerecipe">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/iicon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/iicon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SeasonalFoodActivity"
            android:parentActivityName=".HomeActivity"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".DrinksActivity"
            android:parentActivityName=".HomeActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".SnackActivity"
            android:parentActivityName=".HomeActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".SeafoodActivity"
            android:parentActivityName=".HomeActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".NoodlesActivity"
            android:parentActivityName=".HomeActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".RiceActivity"
            android:parentActivityName=".HomeActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".FishActivity"
            android:parentActivityName=".HomeActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".EggActivity"
            android:parentActivityName=".HomeActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".CurryActivity"
            android:parentActivityName=".HomeActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".ChickenActivity"
            android:parentActivityName=".HomeActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".VegetarianActivity"
            android:parentActivityName=".HomeActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".SoupActivity"
            android:parentActivityName=".HomeActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".NonVegetarianActivity"
            android:parentActivityName=".HomeActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".ForgotPasswordActivity"
            android:parentActivityName=".LoginActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ProfileActivity"
            android:parentActivityName=".HomeActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".HomeActivity"
            android:label="@string/title_activity_home"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SetupActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".RegisterActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".LoginActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".SplashActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>